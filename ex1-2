#include <cstdio>
#include <cstdint>
#include <cmath>

bool isInteger(double x) {
    return std::floor(x) == x;
}

bool isPrime(uint64_t n) {
    if (n < 2) return false;
    if (n % 2 == 0) return n == 2;
    for (uint64_t i = 3; i * i <= n; i += 2) {
        if (n % i == 0) return false;
    }
    return true;
}

int main() {
    double x;
    if (scanf("%lf", &x) != 1) {
        printf("false\n");
        return 0;
    }

    if (!isInteger(x)) {
        printf("false\n");
        return 0;
    }

    uint64_t n = static_cast<uint64_t>(x);
    if (isPrime(n))
        printf("true\n");
    else
        printf("false\n");

    return 0;
}
